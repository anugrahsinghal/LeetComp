{
  "name": "Python with Poetry",
  "customizations": {
    "vscode": {
        "extensions": ["ms-python.python","ritwickdey.LiveServer"],
        "settings": {
            "python.pythonPath": "/usr/local/bin/python"
        }
    }
  },
  
  "postCreateCommand": [
    "pip install poetry",
    "poetry install"
  ]
}